generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DB_URL")
  relationMode = "prisma"
}

model PricePointDaily {
  date  String
  time  DateTime
  value Float
  nr    Int      @id @default(autoincrement())
}

model PricePointHourly {
  date      String
  hour      Int
  minute    Int
  value     Float
  createdAt DateTime @default(now())

  nr Int @id @default(autoincrement())
}
